{
  "name": "بوت استخراج جلسة",
  "description": "Generate Pyrogram And Telethon String Session Easley",
  "logo": "https://telegra.ph/file/27bfcfbb9d72fb3f3a1ca.jpg",
  "keywords": [
    "telegram",
    "bot",
    "python",
    "pyrogram"
  ],
    "buildpacks": [{
    "url": "heroku/python"
  }],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
  "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
  "repository": "https://github.com/xlucifer711/BgtStringBot",
  "env": {
    "ENVIRONMENT": {
      "description": "Don't change ANYTHING.",
      "required": false,
      "value": "ANYTHING"
    },
            "API_ID": {
            "description": "Your App ID",
            "required": true
        },
        "API_HASH": {
            "description": "Your App Hash",
            "required": true
        },
        "MUST_JOIN": {
            "description": "INPUT CHANNEL USERNAME WITHOUT @ ",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "Your Bot Token",
            "required": true
        },
        "MONGO_DB_URL": {
            "description": "Your Mongo DB URL",
            "required": true
        },
        "OWNER_ID": {
            "description": "Your User Id",
            "required": true   
    }
  }
}

